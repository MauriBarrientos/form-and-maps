<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lista de Viajes</title>
</head>
<body>
  <h1>Lista de Viajes</h1>
  <ul>
    <% travels.forEach((travel) => {
      if (travel.estado !== 0) {
    %>
      <li>
        <a href="/travel/<%= travel.id %>" target="_blank">Viaje N<%= travel.id %></a>
      </li>
    <% }
    }); %>
  </ul>
</body>
</html>